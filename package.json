{
  "name": "qualia-backend",
  "version": "1.0.0",
  "description": "Sample Project",
  "main": "dist/server.js",
  "scripts": {
    "start:dev": "tsc-watch --onSuccess \"ts-node-dev -- src/server.ts\"",
    "start": "rm -rf ./dist/ && tsc && node dist/server.js",
    "build": "rm -rf ./dist/ && tsc",
    "build-watch": "rm -rf ./dist/ && tsc-watch",
    "test": "jest --watch --maxWorkers=1",
    "test:cov": "jest --coverage --maxWorkers=1",
    "format": "prettier --write ./**/*.ts",
    "deps": "rm -rf ./node_modules/ && yarn install"
  },
  "author": "Darren Soukup",
  "license": "ISC",
  "dependencies": {
    "awilix": "^5.0.1",
    "colors": "^1.4.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-cors": "^6.0.2",
    "fastify-static": "^4.4.2",
    "knex": "^0.95.12",
    "tedious": "^14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.6.3"
  }
}
